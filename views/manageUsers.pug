extends base
include _userCard

block content 
    main.main 
        h2.heading-secondary.ma-bt-lg.middle Manage Users
        if users.length === 0
            h3.heading-secondary.ma-bt-md.middle No Users found
        else  
            h3.heading-secondary.ma-bt-md.middle= `User Number: ${users.length}`
        .card-container
            if users.length !== 0
                each user in users
                    +userCard(user)                   
                