doctype html 
html 
    head
        block head
            meta(charset='UTF-8')
            meta(name='viewport' content='width=device-width, initial-scale=1.0')
            title Natours | #{title}
            link(rel='stylesheet' href='/css/style.css')
            link(rel='shortcut icon' type='image/png' href='https://dahee-natours-project.s3.amazonaws.com/favicon.png')
            link(rel='stylesheet' href="https://fonts.googleapis.com/css?family=Lato:300,300i,700")
            link(rel="stylesheet" href="https://unpkg.com/leaflet@1.9.4/dist/leaflet.css" integrity="sha256-p4NxAoJBhIIN+hmNHrzRCf9tD/miZyoHS5obTRR9BMY=" crossorigin="")

    body(data-alert=`${alert ? alert : ''}`)
        // HEADER
        include _header
    
        // CONTENT
        block content
            h1 This is a plcae holder heading
    
        // FOOTER
        include _footer

        script(type='module' src='/js/bundled/index.js')
        script(src='https://js.stripe.com/v3/')
        script(src="https://cdn.amcharts.com/lib/5/index.js")
        script(src="https://cdn.amcharts.com/lib/5/xy.js")
        script(src="https://cdn.amcharts.com/lib/5/themes/Animated.js")
        //script(src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js")
        block js

